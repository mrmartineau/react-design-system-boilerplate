{
  "name": "react-design-system-starter-kit",
  "version": "0.0.0",
  "description": "React-based design system start kit",
  "repository": {
    "url": "https://github.com/mrmartineau/react-design-system.git",
    "type": "git"
  },
  "author": {
    "name": "Zander Martineau",
    "url": "https://zander.wtf"
  },
  "license": "MIT",
  "main": "dist/react-design-system-starter-kit.cjs.js",
  "module": "dist/react-design-system-starter-kit.esm.js",
  "browser": {
    "./dist/react-design-system-starter-kit.cjs.js": "./dist/react-design-system-starter-kit.browser.cjs.js",
    "./dist/react-design-system-starter-kit.esm.js": "./dist/react-design-system-starter-kit.browser.esm.js"
  },
  "types": "build/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "playroom": "playroom start",
    "playroom:build": "playroom build",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "format": "prettier --write \"src/**/*.*\" \"*.md\"",
    "clean": "rimraf dist buttons/dist tokens/dist",
    "type-check": "tsc --noEmit",
    "prebuild": "yarn clean",
    "build": "cross-env NODE_ENV=production yarn preconstruct build",
    "watch": "rollup -c -w"
  },
  "preconstruct": {
    "entrypoints": [
      ".",
      "./tokens"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@mdx-js/react": "^1.5.1",
    "@theme-ui/components": "^0.2.49",
    "design-system-utils": "^1.5.0",
    "theme-ui": "^0.2.49"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-react-jsx": "^7.7.0",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@preconstruct/cli": "^1.1.2",
    "@storybook/addon-a11y": "^5.2.6",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-backgrounds": "^5.2.6",
    "@storybook/addon-docs": "^5.2.6",
    "@storybook/addon-info": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-storysource": "^5.2.6",
    "@storybook/addon-viewport": "^5.2.6",
    "@storybook/preset-typescript": "^1.1.0",
    "@storybook/react": "^5.2.6",
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@types/jest": "^24.0.13",
    "@types/storybook__react": "^4.0.1",
    "@types/theme-ui": "^0.2.4",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.23",
    "babel-preset-react-app": "^9.0.2",
    "cross-env": "^6.0.3",
    "cypress": "^3.3.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.0.9",
    "jest": "^24.8.0",
    "jest-emotion": "^10.0.17",
    "lint-staged": "^9.4.3",
    "playroom": "^0.10.1",
    "plop": "^2.3.0",
    "prettier": "^1.17.1",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.4.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.0",
    "storybook-dark-mode": "^0.1.9",
    "storybook-readme": "^5.0.8",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-prettier": "^1.1.0",
    "stylelint-processor-styled-components": "^1.6.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack-dev-server": "^3.4.1"
  }
}
